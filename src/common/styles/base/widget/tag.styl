/*厂家贸易商小图标*/
.factory, .stalls {
    position: relative;
}

.factory:after {
    content: '厂';
    height: 12px;
    width: 12px;
    color: #fff;
    font-size: 8px;
    font-weight: 900;
    background-color: #67d59e;
    line-height: 13px;
    text-align: center;
    position: absolute;
    left: 0;
    top: 3px;
}

.stalls:after {
    content: '贸';
    height: 12px;
    width: 12px;
    color: #fff;
    font-size: 8px;
    font-weight: 900;
    background-color: #ff9119;
    line-height: 13px;
    text-align: center;
    position: absolute;
    left: 0;
    top: 3px;
}