// 2017年7月28日08:33:25  新增色卡
.color-card {
    display: none;
    position: fixed;
    z-index: 6000;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #fff;
    box-shadow: 0 0 1px #e9e9e9;
}

.color-card-main {
    max-height: 320px;
    overflow-y: scroll;
    padding: 0 15px;
}

.color-card-main::-webkit-scrollbar {
    width: 2px;
}

.color-card-main::-webkit-scrollbar-thumb {
    color: #666;
}

.color-card-main::-webkit-scrollbar-track-piece {
    background-color: transparent;
}

.color-card .card-title {
    height: 84px;
    padding: 0 15px;
}

.color-card .card-title .avatar-wrapper {
    height: 84px;
    width: 84px;
    border: 3px solid #fff;
    float: left;
    margin-top: -20px;
}

.color-card .card-title .price-wrapper {
    line-height: 84px;
    float: left;
    margin-left: 12px;
}

.color-card .card-title .price-wrapper .money {
    color: #d40073;
}

.color-card .card-title .arrow {
    float: right;
}

.color-card .card-title .arrow span {
    display: inline-block;
    font-size: 36px;
    color: #909090;
}

.color-card .card-buy-type .name {
    line-height: 48px;
    display: inline-block;
    margin-right: 16px;
}

.color-card .card-buy-type .value {
    display: inline-block;
    height: 22px;
    width: 50px;
    font-size: 12px;
    text-align: center;
    line-height: 23px;
    border: 1px solid #999;
    border-radius: 2px;
    color: #666;
    margin: 13px 0 13px 20px;
}

.color-card .card-buy-type .value.active{
    background-color: #d40073;
    color: #fff;
    border-color: #d40073;
}

.color-card .card-pattern-color {
// height: 90px;
    padding: 12px 0;
    overflow: hidden;
    box-sizing: border-box;
}

.color-card .card-pattern-color img {
    width: 36px;
    height: 36px;
    margin: 14px 10px 0 0;
    border: 2px solid transparent;
}

.color-card .card-pattern-color img.active {
    border-color: #d40073;
}

.card-buy-num span, .card-user-name span , .card-phone span {
    line-height: 45px;
    display: inline-block;
    width: 90px;
}

.card-buy-num input, .card-user-name input, .card-phone input {
    outline: none;
    -webkit-appearance:none;
}

.color-card .submit-tip .text {
    font-size: 16px;
    height: 36px;
    line-height 36px;
    display:inline-block;
}

.card-footer {
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
    background-color: #fff;
    display: flex;
    box-sizing: border-box;
    box-shadow: 0 0 1px #e9e9e9;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
}

.card-footer .cancel {
    flex: 1;
    color: #d40073;
}

.card-footer .confirm{
    flex: 1;
    color: #fff;
    background-color: #d40073;
}

.foot-wrapper-sticky {
    height: 50px;
}

input.invalid::-webkit-input-placeholder {
    color: red;
}

body.modal-open {
    position: fixed;
    width: 100%;
}
