/*footer*/
.footer {
    width: 100%;
    height: 50px;
    text-align: center;
    background: #fff;
    display: flex;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    box-shadow: 0 0 1px #e9e9e9;
}

.footer > div {
    flex: 1;
}

.footer .dress .icon-shiyi, .footer .buy .icon-gouwuche {
    color: #666;
    font-size: 20px;
    padding-top: 8px;
}

.footer .dress .text, .footer .buy .text {
    font-size: 12px;
    line-height: 18px;
}

.footer .call {
    color: #fff;
    background-color: #d40073;
    line-height: 50px;
}

.footer .call .icon-dianhua {
    font-size: 20px;
}

.footer .call .text {
    font-size: 15px;
}


