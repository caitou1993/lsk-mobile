#topSwiper .number {
    color: #fff;
    width: 50px;
    line-height: 20px;
    text-align: center;
    background-color: rgba(7, 17, 27, 0.6);
    border-radius: 10px;
    position: absolute;
    right: 4px;
    bottom: 4px;
    z-index: 3;
}

.desc-wrapper {
    padding: 0 15px 4px 15px;
    background-color: #fff;
    position: relative;
}

.desc-wrapper .left {
    width: 100%;
    overflow: hidden;
}

.desc-wrapper .left .item {
    margin-top: 12px;
}

.desc-wrapper .left .item .name {
    color: #999;
}
.desc-wrapper .left .item .value {
    color: #d40073;
}

.desc-wrapper .left #productNo {
    color: #333;
    font-size: 15px;
}

.desc-wrapper .left .logo {
    display: inline-block;
    height: 12px;
    width: 56px;
    background-image: url("../../images/leisikong_shuiyin@2x.png");
    background-position: center;
    background-size: cover;
    transform: translateY(1px);
}

.desc-wrapper .left .address-wrapper {
    display: flex;
}

.desc-wrapper .left .address-wrapper span {
    display: block;
}

.desc-wrapper .left .address-wrapper .name {
    flex: 0 0 auto;
}

.desc-wrapper .left .address-wrapper #address {
    color: #666;
    flex: 1;
    line-height: 16px;
    //white-space: nowrap;
    //overflow: hidden;
    //text-overflow: ellipsis;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.desc-wrapper .left .color-wrapper {
    display: flex;
}

.desc-wrapper .left .color-wrapper .name {
    width: 59px;
    flex: 0 0 auto;
    margin-bottom: 8px;
}

.desc-wrapper .left .color-wrapper .value {
    flex: 1;
    font-size: 0;
}

.desc-wrapper .left .color-wrapper .value img {
    margin: 0 8px 8px 0;
    height: 50px;
    width: 50px;
    border: 1px solid transparent;
}
    
.desc-wrapper .left .color-wrapper .value img.active {
    border-color: #d40073;
}
    
.desc-wrapper .right {
    position: absolute;
    right: 24px;
    top: 12px;
    text-align: left;
    box-sizing: border-box;
    padding-left: 18px;
    display: none;
}

.desc-wrapper .right .save .text {
    color: #666;
    font-size: 13px;
    line-height: 28px;
}

.desc-wrapper .right .icon-star-small {
    color: #ddd;
    //color: #d40073;
    font-size: 22px;
    position: relative;
    top: 2px;
}


.desc-wrapper .right .icon-star-small.active {
    color: #d40073;
}

.desc-wrapper .right .page-view span {
    color: #999;
    font-size: 12px;
    line-height: 14px;
}

.message {
    height: 72px;
    background-color: #fff;
    display: none;
    position: relative;
    margin-top: 8px;
}
.message .avatar-wrapper {
    flex: 0 0 auto;
    //overflow: hidden;
    margin: 12px 12px 12px 17px;
}
.message .avatar-wrapper .avatar {
    height: 48px;
    width: 48px;
    border-radius: 5px;
}
.message .content-wrapper {
    flex: 1;
    position: relative;
    overflow: hidden;
}
.message .content-wrapper .content {
    //margin-top: 15px;
    width: 95%;
    overflow: hidden;
}
.message .content-wrapper .content .company-name {
    display: block;
    line-height: 72px;
    width: 100%;
    color: #333;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.message .content-wrapper .content .desc {
    display: block;
    color: #666;
    height: 16px;
    width: 100%;
    line-height: 16px;
    margin-top: 16px;
    font-size: 12px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
